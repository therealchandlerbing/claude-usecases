{
  "name": "vianeo-persona-interactive-dashboard",
  "version": "1.0.0",
  "description": "Interactive dashboard for exploring Vianeo persona data with four-layer structure and evidence tracking",
  "main": "src/VianeoPersonaExplorer.tsx",
  "types": "src/types.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:standalone": "vite build --config vite.standalone.config.ts",
    "build:portable": "esbuild src/VianeoPersonaExplorer.tsx --bundle --format=esm --outfile=dist/portable.jsx --loader:.tsx=tsx --loader:.ts=ts",
    "build:all": "npm run build && npm run build:standalone && npm run build:portable",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "vianeo",
    "persona",
    "dashboard",
    "visualization",
    "react",
    "typescript",
    "stakeholder",
    "validation"
  ],
  "author": "360 Innovation Team",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "terser": "^5.27.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/therealchandlerbing/claude-usecases",
    "directory": "skills/vianeo-persona-builder/powerups/interactive-dashboard"
  },
  "bugs": {
    "url": "https://github.com/therealchandlerbing/claude-usecases/issues"
  },
  "homepage": "https://github.com/therealchandlerbing/claude-usecases/tree/main/skills/vianeo-persona-builder/powerups/interactive-dashboard#readme",
  "files": [
    "src",
    "examples",
    "README.md",
    "INTEGRATION.md"
  ]
}
