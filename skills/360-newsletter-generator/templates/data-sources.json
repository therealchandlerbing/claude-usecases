      },
      {
        "name": "project_regional_distribution",
        "source": "asana",
        "method": "Count tasks/projects by tags or names containing region identifiers",
        "categories": ["North America", "Latin America", "Europe", "Asia-Pacific", "Other"],
        "keywords": {
          "North America": ["US", "USA", "United States", "Seattle", "Washington", "Canada"],
          "Latin America": ["Brazil", "SÃ£o Paulo", "Sao Paulo", "Latin America", "LATAM", "Argentina", "Chile"],
          "Europe": ["UK", "Europe", "EU", "Germany", "France"],
          "Asia-Pacific": ["China", "Australia", "Japan", "APAC", "Asia"],
          "Other": ["catch-all for unmatched"]
        }
      },
      {
        "name": "tasks_completed",
        "source": "asana",
        "method": "Count all tasks with completed=true in date range"
      },
      {
        "name": "tasks_modified",
        "source": "asana",
        "method": "Count all tasks modified in date range"
      },
      {
        "name": "partnership_communications",
        "source": "gmail",
        "method": "Count email threads from partner domains"
      },
      {
        "name": "impact_mentions",
        "source": "drive + gmail",
        "method": "Count documents/emails mentioning impact metrics"
      }
    ]
  },

