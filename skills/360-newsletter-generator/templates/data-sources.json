  "date_formats": {
    "description": "Date format requirements for different tools",
    "formats": {
      "asana_date": "YYYY-MM-DD",
      "asana_datetime": "YYYY-MM-DDTHH:MM:SSZ",
      "gmail_date": "YYYY/MM/DD",
      "drive_rfc3339": "YYYY-MM-DDTHH:MM:SSZ",
      "display_date": "Month DD, YYYY",
      "display_range": "Month DD-DD, YYYY"
    },
    "examples": {
      "asana_date": "2025-11-04",
