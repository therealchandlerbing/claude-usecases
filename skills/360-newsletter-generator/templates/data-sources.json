  "asana_queries": {
    "completed_tasks": {
      "description": "Tasks completed during newsletter period",
      "tool": "asana_search_tasks",
      "base_params": {
        "workspace": "{workspace_gid}",
        "completed": true,
        "completed_on_after": "{start_date}",
        "completed_on_before": "{end_date}",
        "opt_fields": "name,notes,completed_at,assignee,custom_fields,projects,tags"
      },
      "additional_filters": {
        "projects_any": "{project_gids_csv}",
        "sort_by": "completed_at",
        "sort_ascending": false
      }
    },
    "modified_tasks": {
      "description": "Tasks modified during newsletter period (active work)",
      "tool": "asana_search_tasks",
      "base_params": {
        "workspace": "{workspace_gid}",
        "modified_on_after": "{start_date}",
        "modified_on_before": "{end_date}",
        "opt_fields": "name,notes,due_date,assignee,custom_fields,projects,tags,completed"
      },
      "additional_filters": {
        "projects_any": "{project_gids_csv}",
        "sort_by": "modified_at",
        "sort_ascending": false
      }
    },
    "project_sections": {
      "description": "Get all sections from a project for organization",
      "tool": "asana_get_project_sections",
      "base_params": {
        "project_id": "{project_gid}"
      }
    },
    "project_details": {
      "description": "Get full project information",
      "tool": "asana_get_project",
      "base_params": {
        "project_id": "{project_gid}",
        "opt_fields": "name,owner,members,custom_fields,notes,due_date,current_status"
      }
    },
    "project_task_counts": {
      "description": "Quick metrics for project health",
      "tool": "asana_get_project_task_counts",
      "base_params": {
        "project_id": "{project_gid}"
      }
    }
  },

  "gmail_queries": {
    "partnership_communications": {
      "description": "Emails from key partner organizations",
      "tool": "search_gmail_messages",
      "query_template": "from:({partner_domains}) after:{start_date_gmail} before:{end_date_gmail}",
      "partner_domains_csv": "spaceplan.com OR genip.com OR sciencelink.org OR nanobioplus.com OR cnen.gov.br OR cmu.edu OR lifesciencewa.org",
      "follow_up": "read_gmail_thread for each result"
    },
    "sent_strategic": {
      "description": "Outbound strategic communications",
      "tool": "search_gmail_messages",
      "query_template": "from:me ({keywords}) after:{start_date_gmail} before:{end_date_gmail}",
      "keywords": "partnership OR agreement OR proposal OR contract OR MOU OR deliverable OR impact OR report"
    },
    "impact_feedback": {
      "description": "Client testimonials and impact reports",
      "tool": "search_gmail_messages",
      "query_template": "({keywords}) after:{start_date_gmail} before:{end_date_gmail}",
      "keywords": "testimonial OR feedback OR impact OR outcome OR thank OR success OR results"
    },
    "board_related": {
      "description": "Board-related communications",
      "tool": "search_gmail_messages",
      "query_template": "(board OR director OR governance) after:{start_date_gmail} before:{end_date_gmail}"
    }
  },

  "drive_queries": {
    "recent_strategic_docs": {
      "description": "Recently modified strategic documents",
      "tool": "google_drive_search",
      "api_query_template": "modifiedTime > '{start_date_rfc3339}' and (name contains 'partnership' or name contains 'agreement' or name contains 'report' or name contains 'deliverable' or name contains 'impact' or name contains 'board')",
      "semantic_query": "partnership agreements reports deliverables impact metrics board updates strategic planning",
      "order_by": "modifiedTime desc",
      "page_size": 20
    },
    "partner_documents": {
      "description": "Documents related to specific partners",
      "tool": "google_drive_search",
      "api_query_template": "modifiedTime > '{start_date_rfc3339}' and fullText contains '{partner_name}'",
      "semantic_query_template": "{partner_name} partnership updates agreements deliverables",
      "iterate_partners": true
    },
    "impact_metrics": {
      "description": "Impact measurement and outcomes documentation",
      "tool": "google_drive_search",
      "api_query_template": "modifiedTime > '{start_date_rfc3339}' and (name contains 'impact' or name contains 'metrics' or name contains 'outcomes' or name contains 'social')",
      "semantic_query": "social impact metrics community outcomes systems change measurement evaluation",
      "order_by": "modifiedTime desc"
    },
    "financial_docs": {
      "description": "Financial and sustainability reports",
      "tool": "google_drive_search",
      "api_query_template": "modifiedTime > '{start_date_rfc3339}' and (name contains 'financial' or name contains 'revenue' or name contains 'budget' or name contains 'forecast')",
      "semantic_query": "financial revenue budget forecast sustainability runway",
      "order_by": "modifiedTime desc"
    }
  },

